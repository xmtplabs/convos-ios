{
  "mcpServers": {
    "XcodeBuildMCP": {
      "command": "npx",
      "args": ["-y", "xcodebuildmcp@latest"],
      "env": {
        "XCODE_PROJECT_PATH": "Convos.xcodeproj"
      }
    },
    "ios-simulator": {
      "command": "npx",
      "args": ["-y", "ios-simulator-mcp@latest"]
    },
    "xmtp-docs": {
      "command": "npx",
      "args": ["-y", "github:xmtp/xmtp-docs-mcp"]
    },
    "graphite": {
      "command": "gt",
      "args": ["mcp"]
    },
    "notion": {
      "type": "http",
      "url": "https://mcp.notion.com/mcp"
    },
    "linear": {
      "type": "http",
      "url": "https://mcp.linear.app/mcp"
    },
    "figma": {
      "type": "http",
      "url": "https://mcp.figma.com/mcp"
    }
  }
}
